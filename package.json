{
  "name": "react-stream-renderer",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "repository": "github:zamotany/react-stream-renderer",
  "author": {
    "name": "Paweł Trysła",
    "email": "pawel.trysla@gmail.com"
  },
  "files": [
    "build",
    "scripts"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "babel src --out-dir build --ignore **/__tests__/**",
    "build:clean": "rimraf build",
    "build:watch": "yarn run build --watch",
    "prepublish": "yarn run build",
    "postinstall": "node ./scripts/export-reconciler.js && yarn run bootstrap:example",
    "bootstrap:example": "yarn link && cd example && yarn && cd ..",
    "flow": "flow",
    "eslint": "eslint src",
    "jest": "jest src",
    "prettier": "prettier --single-quote --trailing-comma all --write \"src/**/*.js\"",
    "test": "npm run eslint && npm run prettier && npm run flow && npm run jest"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "fbjs": "^0.8.16",
    "react": "^16.0.0",
    "react-dom": "16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-callstack-io": "^0.4.1",
    "flow-bin": "^0.54.1",
    "jest": "^21.0.2",
    "np": "^2.16.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "eslintConfig": {
    "extends": "callstack-io"
  }
}
